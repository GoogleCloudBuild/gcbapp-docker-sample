{
  "name": "gatsby-starter-gcn",
  "description": "A starter template to build amazing static websites with Gatsby, Contentful and Netlify",
  "version": "1.2.0",
  "repository": "https://github.com/ryanwiemer/gatsby-starter-gcn",
  "author": "Ryan Wiemer <ryan@ryanwiemer.com>",
  "dependencies": {
    "babel-plugin-styled-components": "^1.8.0",
    "chai": "^4.1.2",
    "contentful-import": "^7.0.3",
    "contentful-management": "^5.1.3",
    "dotenv": "^5.0.1",
    "gatsby": "^2.0.19",
    "gatsby-image": "*",
    "gatsby-plugin-canonical-urls": "*",
    "gatsby-plugin-compression-v2": "^0.1.0",
    "gatsby-plugin-google-tagmanager": "*",
    "gatsby-plugin-manifest": "*",
    "gatsby-plugin-no-sourcemaps": "*",
    "gatsby-plugin-nprogress": "*",
    "gatsby-plugin-react-helmet": "*",
    "gatsby-plugin-remove-serviceworker": "*",
    "gatsby-plugin-sitemap": "*",
    "gatsby-plugin-styled-components": "*",
    "gatsby-remark-prismjs": "*",
    "gatsby-source-contentful": "*",
    "gatsby-source-filesystem": "*",
    "gatsby-transformer-remark": "*",
    "lodash": "^4.17.5",
    "mocha": "^5.2.0",
    "path": "^0.12.7",
    "prismjs": "^1.15.0",
    "query-string": "^6.2.0",
    "react": "^16.5.2",
    "react-dom": "^16.5.2",
    "react-flexbox-grid": "2.1.2",
    "react-hamburger-menu": "1.1.1",
    "react-helmet": "^5.2.0",
    "react-html-parser": "^2.0.2",
    "react-modal": "^3.6.1",
    "react-parallax": "^1.7.0",
    "react-remarkable": "^1.1.3",
    "react-svg": "6.0.5",
    "react-swipe": "^5.1.1",
    "styled-components": "^3.4.9",
    "swipe-js-iso": "^2.0.4",
    "typeface-montserrat": "0.0.54",
    "validate.js": "^0.12.0",
    "whatwg-fetch": "^2.0.4"
  },
  "keywords": [
    "gatsby"
  ],
  "license": "MIT",
  "main": "n/a",
  "scripts": {
    "build": "node --max-old-space-size=4096 node_modules/.bin/gatsby build",
    "build-with-prefix": "node --max-old-space-size=4096 node_modules/.bin/gatsby build --prefix-paths",
    "serve": "gatsby serve",
    "serve-nick": "npm run clean && ./.buildkite/scripts/build-gatsby-prod.sh && npm run serve",
    "clean": "rm -rf public && rm -rf .cache",
    "reinstall": "rm -rf node_modules && npm i",
    "reinstall-hard": "rm -rf node_modules && rm package-lock.json && npm i",
    "start": "npm run clean && npm run build && npm run serve",
    "lint": "eslint .",
    "lint:fix": "npm run lint -- --fix",
    "test": "mocha src/**/*.test.js",
    "setup": "node ./bin/setup.js && echo 'Done'",
    "setup-locales": "node ./bin/setup_locales.js",
    "setup-contentful": "node ./bin/setup_contentful.js",
    "dev": "gatsby develop",
    "dev-nick": "npm run clean && npm i && npm run setup && gatsby develop -H 0.0.0.0 -p 8000",
    "dev-shav": "npm run clean && npm run dev",
    "lint:js": "eslint --ext .js,.jsx .",
    "lint:css": "stylelint 'src/**/*.js'",
    "buildkite-test": "npm run clean && ./.buildkite/scripts/build-gatsby-prod.sh",
    "prepush": "npm run lint && npm test",
    "upload-extension-shopify": "contentful extension update --force --id 5xJz5q9UIgE6OOgSUc2eAo --name Shopify --descriptor contentful/extensions/shopify/extension.json --srcdoc contentful/extensions/shopify/index.html",
    "storybook": "start-storybook -p 6006",
    "build-storybook": "build-storybook",
    "devops:start-build": "node ./devops/start-build.js",
    "devops:step-complete": "node ./devops/step-complete.js",
    "devops:start-deployment": "node ./devops/start-deployment.js",
    "devops:deployment-complete": "node ./devops/deploment-complete.js"
  },
  "devDependencies": {
    "@storybook/addon-actions": "^3.4.11",
    "@storybook/addon-info": "^3.4.11",
    "@storybook/addon-links": "^3.4.11",
    "@storybook/addon-viewport": "^3.4.11",
    "@storybook/addons": "^3.4.11",
    "@storybook/react": "^4.0.0-rc.6",
    "babel-core": "^6.26.3",
    "babel-eslint": "8.2.5",
    "babel-runtime": "^6.26.0",
    "eslint": "^4.19.1",
    "eslint-config-airbnb": "17.0.0",
    "eslint-plugin-babel": "5.1.0",
    "eslint-plugin-import": "^2.14.0",
    "eslint-plugin-jsx-a11y": "6.0.3",
    "eslint-plugin-promise": "3.8.0",
    "eslint-plugin-react": "7.10.0",
    "eslint-plugin-styled-components-config": "0.0.2",
    "husky": "^0.14.3",
    "prettier": "^1.12.1",
    "request": "^2.88.0",
    "request-promise-native": "^1.0.5",
    "stylelint": "9.1.3",
    "stylelint-config-prettier": "^3.2.0",
    "stylelint-config-standard": "^18.2.0",
    "stylelint-config-styled-components": "^0.1.1",
    "stylelint-processor-styled-components": "^1.3.1"
  }
}
